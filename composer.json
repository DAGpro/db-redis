{
    "name": "yiisoft/db-redis",
    "description": "Yii Framework Redis Cache, Session and ActiveRecord extension",
    "keywords": ["yii", "redis", "active-record", "cache", "session"],
    "type": "library",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii-redis/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii-redis"
    },
    "authors": [
        {
            "name": "Carsten Brandt",
            "email": "mail@cebe.cc"
        }
    ],
    "require": {
        "yiisoft/yii-core": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "yiisoft/di": "dev-master",
        "yiisoft/log": "dev-master",
        "yiisoft/cache": "dev-master"
    },
    "autoload": {
        "psr-4": { "yii\\redis\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "yiiunit\\extensions\\redis\\": "tests/"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
